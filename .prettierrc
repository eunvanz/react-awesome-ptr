{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "printWidth": 90,
  "proseWrap": "preserve",
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^(?![.~]).*(?<!.svg|.png|.css|.scss)$",
    "^~.*(?<!.svg|.png|.css|.scss)$",
    "^\\..*(?<!.svg|.png|.css|.scss)$",
    "^(?![.~]).*(.css|.scss)$",
    ".*(.css|.scss)$",
    ".*"
  ]
}
